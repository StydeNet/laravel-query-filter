{
    "name": "styde/laravel-query-filter",
    "description": "Apply filters to your queries using eloquent.",
    "keywords": [
        "laravel-query-filter"
    ],
    "homepage": "https://github.com/StydeNet/laravel-query-filter",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Luis Andr√©s Arce C.",
            "email": "luilliarcec@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Styde\\LaravelQueryFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Styde\\LaravelQueryFilter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Styde\\LaravelQueryFilter\\LaravelQueryFilterServiceProvider"
            ]
        }
    }
}
